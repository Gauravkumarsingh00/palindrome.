#include<bits/stdc++.h>
using namespace std;
int main()
{
    int a;
    printf("enter the number");
    scanf("%d",&a);
    int t=a;
    
    int temp=0;
    while (a!=0)
    {
        
    
    
    temp= temp *10+a%10;
    a=a/10;
    }
    if (temp==t){
        cout<<"it is palindrome"<<endl;

    }
    else{
        cout<<"not palindrome";
    }

return 0;
}
